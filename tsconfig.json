{
  "compilerOptions": {
    "allowJs": false,
    "allowUnreachableCode": false,
    "declaration": true,
    "module": "commonjs",
    "newLine": "LF",
    "noEmitOnError": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "removeComments": true,
    "sourceMap": true,
    "suppressImplicitAnyIndexErrors": true,
    "target": "es6"
  }
}
